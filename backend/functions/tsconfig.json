{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "importHelpers": true,
        "module": "commonjs",
        "noImplicitReturns": true,
        // "noUnusedLocals": true,
        "outDir": "lib",
        "sourceMap": true,
        "strict": true,
        "target": "es2017",
        "types": [
            "facebook-js-sdk"
        ],
        "typeRoots": [
            "./node_modules/@types"
        ]
    },
    "compileOnSave": true,
    "include": [
        "src",
        "index.js"
    ],
}